<div id="signin-page" class="container">
  <h1>Quiz Master</h1>
  <img src="<%= asset_path 'qm2_logo.jpg'%>">

  <h2>Sign In</h2>

  <form id="signin-form">
    <input type="text" name="email" placeholder="email">
    <input type="password" name="password" placeholder="password">
    <input type="submit" name="Sign in" class="btn btn-success">
    <a href="/" class="btn btn-info btn-default">Back</a>
  </form>

  <div id="signin-form-message"></div>

</div>
